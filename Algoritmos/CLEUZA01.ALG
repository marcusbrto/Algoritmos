Algoritmo "Cleuza01"

Var

 ano_atual,ano_nasci,resultado : inteiro

Inicio
         Escreval ("Em que ano nos estamos? ")
                  leia (ano_atual)
         Escreval ("Em que ano eu nasci? ")
                  leia (ano_nasci)
         resultado <- (ano_atual - ano_nasci)
                   Escreva ("Então eu tenho",resultado," anos!")
Fimalgoritmo