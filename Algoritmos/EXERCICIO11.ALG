Algoritmo "exercicio11"
Var
    a, b, c, delta : real
Inicio
       escreva("Digite número A: ")
       leia(a)
       escreva("Digite número B: ")
       leia(b)
       escreva("Digite número C: ")
       leia(c)
       delta <- (b * b) - (4 * a * c)
       escreva("Valor de Delta é ", delta)
Fimalgoritmo