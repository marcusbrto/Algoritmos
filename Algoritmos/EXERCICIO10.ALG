Algoritmo "exercicio10"
Var
    largura, altura, area, qtinta : real
Inicio
      escreva("Qual a largura da parede? ")
      leia(largura)
      escreva("Qual a altura da parede? ")
      leia(altura)
      area <- largura * altura
      qtinta <- area / 2
      escreval("Area: ",area," metros quadrados.")
      escreval("Quantidade de Tinta: ",qtinta," litros.")
Fimalgoritmo